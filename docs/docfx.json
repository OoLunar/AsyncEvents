{
    "build": {
        "globalMetadata": {
            "_appName": "OoLunar.AsyncEvents",
            "_appTitle": "OoLunar.AsyncEvents"
        },
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "xref": [
            "https://oolunar.github.io/AsyncEvents/xrefmap.yml"
        ],
        "output": "_site",
        "template": [
            "default",
            "modern"
        ]
    },
    "metadata": [
        {
            "src": [
                {
                    "src": "../",
                    "files": [
                        "src/OoLunar.OoLunar.AsyncEvents.csproj"
                    ]
                }
            ],
            "dest": "api",
            "properties": {
                "LangVersion": "preview"
            }
        }
    ]
}